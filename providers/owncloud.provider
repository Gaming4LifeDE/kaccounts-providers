<?xml version="1.0" encoding="UTF-8"?>
<provider id="owncloud">
  <name>ownCloud</name>
  <icon>owncloud</icon>
  <translations>kde-account-plugins</translations>
    <template>
        <group name="auth">
            <setting name="method">password</setting>
            <setting name="mechanism">password</setting>
        </group>
        <group name="server">
            <setting name="url"></setting>
        </group>
    </template>
</provider>